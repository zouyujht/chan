# template_reits.py 配置文件

# 数据源 (REITs通常使用不复权数据)
data_src: "custom:OfflineDataAPI.CStockFileReader"

# CChan 配置
chan_config:
  cal_demark: true

# 绘图配置 (只绘制日线相关图表)
plot_config:
  plot_kline: true
  plot_bi: true
  plot_seg: true
  plot_zs: true
  plot_bsp: true
  plot_segbsp: true
  plot_macd: false
  plot_demark: true

# 绘图参数 (默认为空)
plot_para: {}